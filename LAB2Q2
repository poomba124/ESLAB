	AREA RESET,DATA,READONLY
	EXPORT __Vectors
__Vectors
	DCD 0X10001000
	DCD Reset_Handler
	ALIGN
	AREA mycode,CODE,READONLY
	ENTRY
	EXPORT Reset_Handler
Reset_Handler
	LDR R0,=DST
	LDR R1,=DST+36
	MOV R4,#5
LOOP
	LDR R2,[R0]
	LDR R3,[R1]
	
	STR R2,[R1],#-4
	STR R3,[R0],#4
	
	SUBS R4,#1
	BNE LOOP
STOP B STOP
;SRC DCD 1,2,3,4,5,6,7,8,9,0XA
	AREA DATASEG,DATA,READWRITE
DST DCD 1,2,3,4,5,6,7,8,9,0XA
	END
