	AREA RESET,DATA,READONLY
	EXPORT __Vectors
__Vectors
	DCD 0X40001000
	DCD Reset_Handler
	ALIGN
	AREA MYCODE,CODE,READONLY
	ENTRY
	EXPORT Reset_Handler
Reset_Handler
	LDR R0,=NUM
	LDR R1,[R0]
	MOV R2,R1
	AND R1,#0XF ;LOWER NIBBLE
	LSR R2,#4 ;UPPER NIBBLE
	MOV R3,#0XA
	MLA R4,R3,R2,R1
	LDR R5,=RESULT
	STR R4,[R5]
STOP B STOP
NUM DCD 0X99
	AREA MYDATA,DATA,READWRITE
RESULT DCD 0
	END
		
