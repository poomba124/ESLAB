	AREA RESET,DATA,READONLY
	EXPORT __Vectors
__Vectors
	DCD 0X10001000
	DCD Reset_Handler
	ALIGN
	AREA MYCODE,CODE,READONLY
	ENTRY
	EXPORT Reset_Handler
Reset_Handler
	LDR R1,=NUM1
	LDR R2,=NUM2
	LDR R7,=RESULT
	MOV R3,#4
	MOVS R8,#0
LOOP
	LDR R4,[R1],#4
	LDR R5,[R2],#4
	ADCS R6,R4,R5
	STR R6,[R7],#4
	SUB R3,#1
	TEQ R3,#0
	BNE LOOP
NUM1 DCD 0x11111111,0x22222222,0x33333333,0x44444444
NUM2 DCD 0X55555555,0X66666666,0X77777777,0X88888888
	AREA MYDATA,DATA,READWRITE
RESULT DCD 0
	
