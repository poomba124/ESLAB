	AREA RESET,DATA,READONLY
	EXPORT __Vectors
__Vectors
	DCD 0X10001000
	DCD Reset_Handler
	ALIGN
	AREA MYCODE,CODE,READONLY
	ENTRY
	EXPORT Reset_Handler
Reset_Handler
	MOV R0,#10
	LDR R1,=RESULT
	LDR R2,=NUMS
	MOV R5,#0
LOOP
	LDR R3,[R2]
	ADD R5,R3
	SUBS R0,#1
	ADD R2,#4
	BNE LOOP
	STR R5,[R1]
STOP B STOP
NUMS DCD 1,2,3,4,5,6,7,8,9,0XA
	AREA MYDATA,DATA,READWRITE
RESULT DCD 0
	
